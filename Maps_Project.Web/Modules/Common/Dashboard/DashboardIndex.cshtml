<script type="module">
    import { DashboardPage } from '@Html.ResolveWithHash(ESM.Modules.Common.Dashboard.DashboardPage)';

    @{
        // assume this is coming from the Model
        var visitorsByRegion = new Dictionary<string, int>
        {
            ["US"] = 398,
            ["SA"] = 400,
            ["CA"] = 1000,
            ["DE"] = 500,
            ["FR"] = 760,
            ["CN"] = 300,
            ["AU"] = 700,
            ["BR"] = 600,
            ["IN"] = 800,
            ["GB"] = 320,
